{
  "name": "backend-supabase",
  "version": "1.0.0",
  "scripts": {
    "build": "npm install && npm run db:init && tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "postinstall": "prisma generate",
    "db:init": "prisma db push && ts-node src/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "prisma": "^6.15.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
